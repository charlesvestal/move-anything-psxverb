{
  "id": "psxverb",
  "name": "PSX Verb",
  "version": "0.1.0",
  "description": "PlayStation 1 SPU reverb emulation",
  "author": "Charles Vestal",
  "dsp": "psxverb.so",
  "api_version": 1,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "chain_params": [
      {"key": "preset", "name": "Preset", "type": "int", "min": 0, "max": 5, "default": 4},
      {"key": "decay", "name": "Decay", "type": "float", "min": 0.0, "max": 1.0, "default": 0.7, "step": 0.05},
      {"key": "mix", "name": "Mix", "type": "float", "min": 0.0, "max": 1.0, "default": 0.35, "step": 0.05}
    ]
  }
}
