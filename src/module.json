{
  "id": "psxverb",
  "name": "PSX Verb",
  "abbrev": "PX",
  "version": "0.3.2",
  "description": "PlayStation 1 SPU reverb emulation",
  "author": "Charles Vestal",
  "license": "MIT",
  "dsp": "psxverb.so",
  "api_version": 2,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "PSX Verb",
          "params": ["preset", "decay", "mix", "input_gain", "reverb_level"],
          "knobs": ["decay", "mix", "input_gain", "reverb_level"]
        }
      }
    },
    "chain_params": [
      {
        "key": "preset",
        "name": "Preset",
        "type": "enum",
        "options": ["Room", "Studio S", "Studio M", "Studio L", "Hall", "Space Echo"],
        "default": 4
      },
      {
        "key": "decay",
        "name": "Decay",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.7,
        "step": 0.05
      },
      {
        "key": "mix",
        "name": "Mix",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.35,
        "step": 0.05
      },
      {
        "key": "input_gain",
        "name": "Input",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.5,
        "step": 0.05
      },
      {
        "key": "reverb_level",
        "name": "Level",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.5,
        "step": 0.05
      }
    ]
  }
}
