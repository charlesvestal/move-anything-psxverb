{
    "name": "Line In + PSX Verb",
    "version": 1,
    "chain": {
        "input": "both",
        "synth": {
            "module": "linein",
            "config": {}
        },
        "audio_fx": [
            {
                "type": "psxverb",
                "params": {
                    "preset": 4,
                    "decay": 0.8,
                    "input_gain": 0.5,
                    "level": 0.5,
                    "mix": 0.4
                }
            }
        ],
        "knob_mappings": [
            {"cc": 72, "target": "fx1", "param": "preset"},
            {"cc": 73, "target": "fx1", "param": "input_gain"},
            {"cc": 74, "target": "fx1", "param": "decay"},
            {"cc": 75, "target": "fx1", "param": "level"},
            {"cc": 76, "target": "fx1", "param": "mix"}
        ]
    }
}
